// -------------------------------------------------------
// プレースホルダー セレクター
// ※コンパイル後のcssには残らない
// -------------------------------------------------------
$htc
	position relative
	behavior url(/assets/js/PIE.htc)
	// zoom 1

$cl
	overflow hidden
	\/zoom 1

$clearfix
	\/zoom 1
	&:after
		display table
		content ""
		clear both

// -------------------------------------------------------
// リスト
// -------------------------------------------------------
$notes1em
	padding-left 1em
	text-indent -1em

$notes2em
	padding-left 2em
	text-indent -2em

